  "scripts": {
    "setup-hooks": "cpy ./githooks/* .git/hooks",
    "setup-packages": "yarn",
    "setup": "yarn setup-packages && yarn setup-hooks",
    "lint-prod": "tslint -c tslint.prod.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint-dev": "tslint -c tslint.dev.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint": "run-s lint-dev",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build-clear": "rimraf ./build",
    "build-compile": "cross-env NODE_ENV=production webpack",
    "build": "run-s lint-prod build-clear build-compile",
    "test-prod": "run-s lint-prod build-compile",
    "test-dev": "run-s lint-dev build-compile",
    "test": "run-s test-dev",
    "story": "start-storybook -p 3001 -c .storybook"
  },
